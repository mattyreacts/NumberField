{
  "name": "@mattyreacts/numberfield",
  "version": "1.0.1",
  "description": "A number text field based on @mui/material/TextField that only allows valid numbers as input",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest --env=jsdom"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattyreacts/NumberField.git"
  },
  "keywords": [
    "MUI",
    "numberfield",
    "number",
    "input",
    "decimal"
  ],
  "author": "Mathew Crothers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattyreacts/NumberField/issues"
  },
  "homepage": "https://github.com/mattyreacts/NumberField#readme",
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.7"
  }
}
